<?xml version="1.0" encoding="US-ASCII" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="Clients-tf">
    <default-activity>List</default-activity>
    <view id="List">
      <page>/aeg.cw.Clients/list.amx</page>
    </view>
    <view id="Details">
      <page>/aeg.cw.Clients/details.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>List</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>details</from-outcome>
        <to-activity-id>Details</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>Details</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>list</from-outcome>
        <to-activity-id>List</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
